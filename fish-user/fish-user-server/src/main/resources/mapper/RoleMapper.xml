<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!--suppress SqlNoDataSourceInspection, SqlResolve -->
<mapper namespace="cn.devifish.cloud.user.server.mapper.RoleMapper">

    <resultMap id="baseResultMap" type="cn.devifish.cloud.user.common.entity.Role">
        <id column="u_id" property="id"/>
        <result column="name" property="name"/>
        <result column="code" property="code"/>
        <result column="description" property="description"/>
        <result column="authorities" property="authorities"/>
        <result column="system_flag" property="systemFlag"/>
        <result column="u_create_time" property="createTime"/>
        <result column="u_update_time" property="updateTime"/>
    </resultMap>

    <sql id="baseSql">
        `u`.id AS u_id,
        `u`.name,
        `u`.code,
        `u`.description,
        `u`.authorities,
        `u`.system_flag,
        `u`.create_time AS u_create_time,
        `u`.update_time AS u_update_time
    </sql>
</mapper>